body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

body
  background-color: #ddd
  font-family: myriad pro
  color: #333
*
  margin: 0
  padding: 0
td, tr, table
  vertical-align: top
#canvas_cell 
  float: right
  padding: 0
  margin: 0
  background-color: #ccc
  width: 66em
  height:100em
#ribbon 
  height:100%;
  background-color: #f9f9f9;
  padding: 1em;
#ribbon #theme-selector 
  width: 9.5em
  padding: 0.5em
  padding-bottom: 0.33em
  margin-bottom: 1em
  font-weight: bold
  font-size: 18pt
  background-color: rgba(0,0,0,0.2)

.theme-styles-headers 
  font-size: 12pt
  border-bottom: 0.08em solid #DDD
  margin-bottom: 1em
  margin-top: 1.8em

#add-theme-style-button  
  float: right
  font-weight: bold
  font-family: arial
  margin-top: -0.3em
  color: rgb(10,200,33)
  border: none
  background: 0
  font-size: 1.5em

.theme-style 
  width: 18em
  padding: 0.5em
  padding-bottom: 0.33em
  margin-bottom: 1em
  font-size: 10pt
  background-color: rgba(0,0,0,0.1)

#ribbon-shadow 
  position: absolute
  background-color: #bbb
  padding-left: 0.22em
  padding-right: 0.22em
  height: 2000px
  float: left
  width: 18em


canvas 
  background-color: #eee
 width: 1000px
  height: 2000px
#style-editor .close-button 
  width: 1em
  font-size: 1.4em
  padding: 0.6em 
  height: 1em
  position: absolute
  right: 0
  top: 0 
#style-editor 
  position: fixed
  font-weight: bold
  left: 4em
  top: 2em
  background-color: rgba(57, 133, 250, 0.7)	
  color: #222
  font-size: 12pt
  width:54em

.style-editor-body 
  padding: 1em
  font-size: 12pt
  width: 100%
  height: 20em

.left-side 
  width: 28em
  float: left

.right-side 
  float: left 

.four-sides-legend  
  padding: 1em
  margin-bottom: 1.5em

.four-sides-legend .four-sides-legend-item  
  width: 6em
  float:left


.four-sides-distances  

  margin-bottom: 1.8em 
.four-sides-distances .label  
  margin-bottom: 0.2em
  font-size: 12pt
  font-weight: bold	
.four-sides-distances .distance-input  
  float: left 
  margin-right: 0.75em
.four-sides-distances .distance-input input 
  border: 0
  width: 3em 
.four-sides-distances .distance-input select 
  font-size: 10pt
  width: 3em 
.small-sample 
  opacity: 0.8
  width: 24em
  height: 10em
  margin: 0
  background-color: #eee
  margin-bottom: 1em

.block-type-identifier 
  font-weight: bold
  font-size: 12pt
  background-color: rgba(0,0,0,0.2)
  border: none
  padding: 0.33em
  margin-top: 1em 
  margin-left: 1em 

.font-row-one, .font-row-two 
  margin-bottom: 0.8em
  height: 1.30em
  padding-left: 0.25em


.font-row-one div, .font-row-one input 
  float: left 
.font-row-one input  
  width: 12em 
  margin-right: 0.7em 
  margin-top: -0.4em
.font-row-One input.distance  
  width: 4em
.font-row-one select  
  width: 4em 
  top: -0.45em 
  position: relative
.font-label  
  width: 8em
.font-row-two input 
  width: 2.7em 
  height: 3em 
  margin-right: 1em
  border: 0
  padding: 0 
  background: none
  background-color: rgba(0,0,0,0.33) 

.align-row 
  padding-top: 1.8em
  margin-bottom: 0
  padding-bottom: 0 
.right-side-row .label, .left-side-row .label, .row-label 
  width: 10em
  float: left 
.right-side-row, left-side-row 
  padding: 0
  margin-bottom: 1.5em 
.bottom-section 
  position: relative
  padding: 1em
  margin-top: 17em
  left: -1em
  height:10em
  background-color: rgba(0,0,0,0.2) 